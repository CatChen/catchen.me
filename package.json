{
  "name": "catchen.me",
  "version": "1.0.0",
  "description": "This is the source code that can be compiled into the static site behind catchen.me.",
  "private": true,
  "dependencies": {
    "autoprefixer": "^9.8.6",
    "harp": "^0.33.0",
    "postcss-cli": "^7.1.2"
  },
  "devDependencies": {
    "husky": "6",
    "prettier": "2.2.1",
    "pretty-quick": "^3.1.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "harp compile . && npx postcss www/css/*.css --use autoprefixer -d www/css/",
    "start": "harp server .",
    "prettier": "prettier --write public/**/*.json",
    "postinstall": "husky install"
  },
  "prettier": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CatChen/catchen.me.git"
  },
